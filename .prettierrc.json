{
  "printWidth": 100,
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "^react(/.*)?$",
    "^next(/.*)?$",
    "<THIRD_PARTY_MODULES>",
    "",
    "^types$",
    "^@/types/.*$",
    "^@/config/.*$",
    "^@/lib/.*$",
    "^@/hooks/.*$",
    "^@/components/ui/.*$",
    "^@/components/.*$",
    "^@/styles/.*$",
    "",
    "<TYPES>^node:",
    "<TYPES>",
    "<TYPES>^@/types(/.*)?$",
    "<TYPES>^\\.",
    "",
    "^\\."
  ],
  "tailwindStylesheet": "./app/ui/global.css",
  "tailwindFunctions": ["cva", "tv"]
}
